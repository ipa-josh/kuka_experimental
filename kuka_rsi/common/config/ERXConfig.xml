<ROOT>
   <CONFIG>
      <IP_NUMBER>172.31.1.99</IP_NUMBER>    <!-- IP Number of the socket !-->
      <PORT>49150</PORT>                   <!-- Port Number of the socket !-->
      <PROTOCOL>UDP</PROTOCOL>            <!-- TCP or UDP, Protocol of the socket !-->
      <SENTYPE>ImFree</SENTYPE>           <!-- The name of your system send in <Sen Type="" > !-->
      <PROTCOLLENGTH>Off</PROTCOLLENGTH>  <!-- On or Off, Send the length of data in bytes before XML data begins!-->
      <ONLYSEND>FALSE</ONLYSEND>          <!-- TRUE means the client don't expect a answer. Do not send anything to robot!-->
   </CONFIG>
   
   <SEND>
      <ELEMENTS>
         <ELEMENT TAG="DEF_AIPos" TYPE="DOUBLE" INDX="INTERNAL" UNIT="0" />
         
         <ELEMENT TAG="DIGOUT1" TYPE="BOOL" INDX="12" UNIT="0" HOLDON="1" />
         <ELEMENT TAG="DIGOUT2" TYPE="BOOL" INDX="12" UNIT="0" HOLDON="1" />
         <ELEMENT TAG="DIGOUT3" TYPE="BOOL" INDX="12" UNIT="0" HOLDON="1" />
      </ELEMENTS>
   </SEND>
   
   <RECEIVE>
      <ELEMENTS>
         <ELEMENT TAG="AK.A1" TYPE="DOUBLE" INDX="7" UNIT="0" HOLDON="1" />
         <ELEMENT TAG="AK.A2" TYPE="DOUBLE" INDX="8" UNIT="0" HOLDON="1" />
         <ELEMENT TAG="AK.A3" TYPE="DOUBLE" INDX="9" UNIT="0" HOLDON="1" />
         <ELEMENT TAG="AK.A4" TYPE="DOUBLE" INDX="10" UNIT="0" HOLDON="1" />
         <ELEMENT TAG="AK.A5" TYPE="DOUBLE" INDX="11" UNIT="0" HOLDON="1" />
         <ELEMENT TAG="AK.A6" TYPE="DOUBLE" INDX="12" UNIT="0" HOLDON="1" />
         
         <ELEMENT TAG="DIGOUT1" TYPE="BOOL" INDX="12" UNIT="0" HOLDON="1" />
         <ELEMENT TAG="DIGOUT2" TYPE="BOOL" INDX="12" UNIT="0" HOLDON="1" />
         <ELEMENT TAG="DIGOUT3" TYPE="BOOL" INDX="12" UNIT="0" HOLDON="1" />
      </ELEMENTS>
   </RECEIVE>
</ROOT>
